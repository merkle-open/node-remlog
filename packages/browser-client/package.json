{
	"name": "@namics/remlog-browser-client",
	"version": "0.2.5",
	"description": "",
	"main": "./dist/browser-client.js",
	"scripts": {
		"test": "exit 0",
		"o-test": "ava --tap | tap-diff",
		"o-test:watch": "ava --no-cache --watch",
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"commit": "./node_modules/.bin/git-cz",
		"generate-changelog":
			"./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s  -r 0 --lernaPackage @namics/remlog-browser-client && git add CHANGELOG.md"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/namics/node-remlog.git"
	},
	"keywords": [
		"remote",
		"logging",
		"remlog",
		"node",
		"debug",
		"transport",
		"@namics/remlog",
		"remotely",
		"server"
	],
	"author": "Jan Biasi <biasijan@gmail.com>",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"babel-runtime": "6.26.0"
	},
	"devDependencies": {
		"ava": "0.24.0",
		"babel-cli": "6.26.0",
		"babel-minify": "0.2.0",
		"babel-plugin-transform-class-properties": "6.24.1",
		"babel-preset-env": "1.6.1",
		"babel-preset-es2015": "6.24.1",
		"babel-register": "^6.26.0",
		"browser-env": "3.2.4",
		"commitizen": "3.0.2",
		"conventional-changelog-cli": "1.3.5",
		"cz-conventional-changelog": "2.1.0",
		"expose-loader": "0.7.4",
		"rollup": "0.55.0",
		"rollup-plugin-babel": "3.0.3",
		"rollup-plugin-json": "2.3.0",
		"rollup-plugin-node-resolve": "3.0.2",
		"rollup-plugin-uglify": "3.0.0",
		"rollup-watch": "4.3.1",
		"tap-diff": "0.1.1"
	},
	"ava": {
		"babel": "inherit",
		"require": ["babel-register", "./test/helpers/setup-browser-env.js"]
	},
	"babel": {
		"presets": ["@ava/stage-4"]
	}
}
